{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "Run JS on whole website",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "Main.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ]
}